		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="mifos"/>
		<query language="SQL"><![CDATA[SELECT DISTINCT 
    o.name AS Office_Branch,
    c.account_no AS Client_Account_No,
    c.display_name AS Name,
    CASE 
        WHEN c.status_enum = 300 THEN 'Active'
        ELSE 'Unknown'
    END AS Status,
    DATE_FORMAT(c.activation_date, '%Y-%m-%d') AS Activation_Date,
    COALESCE(c.external_id, 'N/A') AS External_Id
FROM 
    m_client c
JOIN 
    m_office o ON c.office_id = o.id
ORDER BY 
    o.name, c.display_name;
]]></query>
